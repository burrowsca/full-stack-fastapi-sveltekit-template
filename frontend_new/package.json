{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"lint": "biome check --write --unsafe --no-errors-on-unmatched --files-ignore-unknown=true ./",
		"preview": "vite preview",
		"generate-client": "openapi-ts --input ./openapi.json --output ./src/client --client fetch",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.2",
		"@hey-api/openapi-ts": "^0.47.2",
		"@skeletonlabs/skeleton": "2.10.1",
		"@skeletonlabs/tw-plugin": "0.4.0",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/kit": "^2.5.17",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.13",
		"@types/node": "20.14.7",
		"autoprefixer": "10.4.19",
		"postcss": "8.4.38",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.1",
		"tailwindcss": "3.4.4",
		"tslib": "^2.6.3",
		"typescript": "^5.5.2",
		"vite": "^5.3.1",
		"vite-plugin-tailwind-purgecss": "0.3.3"
	},
	"type": "module"
}
